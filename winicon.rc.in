//
// Icon
//

// Icon with lowest ID value placed first to ensure application icon
// remains consistent on all systems.
IDI_ICON1               ICON            DISCARDABLE        "${CMAKE_CURRENT_SOURCE_DIR}/assets/favicon.ico"

#if defined(UNDER_CE)
    #include <winbase.h>
#else
    #include <winver.h>
#endif

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @RobotControlManager_VERSION_MAJOR@,@RobotControlManager_VERSION_MINOR@,@RobotControlManager_VERSION_PATCH@,@RobotControlManager_VERSION_TWEAK@
 PRODUCTVERSION @RobotControlManager_VERSION_MAJOR@,@RobotControlManager_VERSION_MINOR@,@RobotControlManager_VERSION_PATCH@,@RobotControlManager_VERSION_TWEAK@
 FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "Open Source Group"
            VALUE "FileDescription", "${RobotControlManager_DESCRIPTION}"
            VALUE "FileVersion", "@RobotControlManager_VERSION_MAJOR@.@RobotControlManager_VERSION_MINOR@.@RobotControlManager_VERSION_PATCH@.@RobotControlManager_VERSION_TWEAK@"
            VALUE "InternalName", "RobotControl.exe"
            VALUE "LegalCopyright", "MIT"
            VALUE "LegalTrademarks", "Open Source Software"
            VALUE "OriginalFilename", "appRobotControl.exe"
            VALUE "ProductName", "${PROJECT_NAME}"
            VALUE "ProductVersion", "@RobotControlManager_VERSION_MAJOR@.@RobotControlManager_VERSION_MINOR@.@RobotControlManager_VERSION_PATCH@.@RobotControlManager_VERSION_TWEAK@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END

